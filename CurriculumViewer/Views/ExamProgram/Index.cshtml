@model IEnumerable<ExamProgram>

@{
	ViewData["Title"] = "Index";
}

@await Component.InvokeAsync(ViewComponentConstants.BreadCrumbs)

<div class="row">
	<div class="col s12">
		<h2>Examenprogramma's</h2>
		@await Html.PartialAsync("_CreateButton", "Examenprogramma")
		<app-data-table data-records='@Html.Raw(Json.Serialize(Model.ToList()))'
						data-limit="10"
						data-columns="name:Examenprogramma"
						data-url_details="@Url.Action("details", "ExamProgram", null)" />
	</div>
</div>