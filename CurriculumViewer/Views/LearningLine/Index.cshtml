@model IEnumerable<LearningLine>

@{
	ViewData["Title"] = "Index";
}
@await Component.InvokeAsync(ViewComponentConstants.BreadCrumbs)
<div class="row">
	<div class="col s12">
		<h2>Leerlijnen</h2>
		@await Html.PartialAsync("_CreateButton", "Leerlijn")
		<app-data-table data-records='@Html.Raw(Json.Serialize(Model.ToList()))'
						data-limit="10"
						data-columns="name:Naam"
						data-url_details="@Url.Action("details", "learningline", null)" />
	</div>
</div>